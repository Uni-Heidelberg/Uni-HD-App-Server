{
    "name": "MensaSection",
    "base": "PersistedModel",
    "indexes": {
        "uniqueIndex": {
            "columns": "title, mensaId",
            "kind": "unique"
        }
    },
    "properties": {
        "title": {
            "type": "string",
            "required": true,
            "length": 20,
            "null": false
        }
    },
    "validations": [],
    "relations": {
        "mensa": {
            "type": "belongsTo",
            "model": "Mensa",
            "foreignKey": "mensaId"
        },
        "menus": {
            "type": "hasMany",
            "model": "MensaDailyMenu",
            "foreignKey": "sectionId"
        }
    },
    "acls": [
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "EXECUTE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        }
    ],
    "methods": []
}
