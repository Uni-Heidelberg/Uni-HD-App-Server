{
    "name": "CanteenMeal",
    "base": "PersistedModel",
    "indexes": {
        "uniqueIndex": {
            "columns": "title, priceStud, priceBed, priceGuest",
            "kind": "unique"
        }
    },
    "properties": {
        "title": {
            "type": "string",
            "required": true,
            "null": false,
            "length": 200
        },
        "priceStud": {
            "type": "number",
            "required": true,
            "null": false,
            "dataType": "double"
        },
        "priceBed": {
            "type": "number",
            "required": true,
            "null": false,
            "dataType": "double"
        },
        "priceGuest": {
            "type": "number",
            "required": true,
            "null": false,
            "dataType": "double"
        },
        "vegetarian": {
            "type": "boolean",
            "default": false,
            "null": false
        }
    },
    "validations": [],
    "relations": {
        "menus": {
            "type": "hasAndBelongsToMany",
            "model": "CanteenDailyMenu",
            "foreignKey": "menuId"
        }
    },
    "acls": [
