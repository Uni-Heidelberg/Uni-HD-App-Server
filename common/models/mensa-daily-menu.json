{
    "name": "MensaDailyMenu",
    "base": "PersistedModel",
    "indexes": {
        "uniqueIndex": {
            "columns": "date, sectionId",
            "kind": "unique"
        }
    },
    "properties": {
        "date": {
            "type": "date",
            "required": true,
            "null": false
        }
    },
    "validations": [],
    "relations": {
        "section": {
            "type": "belongsTo",
            "model": "MensaSection",
            "foreignKey": "sectionId"
        },
        "meals": {
            "type": "hasAndBelongsToMany",
            "model": "MensaMeal",
            "foreignKey": "mealId"
        }
    },
    "acls": [
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "EXECUTE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        }
    ],
    "methods": []
}
